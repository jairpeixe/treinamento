   vel_hor = 0;
}

x += vel_hor;

// Colisão Vertical
if(place_meeting(x, y + vel_vert, obj_parede))
{
   while(!place_meeting(x,y + sign(vel_vert), obj_parede))
   {
        y += sign(vel_vert);
   }
   vel_vert = 0;
}

y += vel_vert;

